{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "film-o-rama": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps\\film-o-rama",
      "sourceRoot": "apps\\film-o-rama\\src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist\\apps\\film-o-rama",
            "index": "apps/film-o-rama/src/index.html",
            "main": "apps/film-o-rama/src/main.ts",
            "polyfills": "apps/film-o-rama/src/polyfills.ts",
            "tsConfig": "apps\\film-o-rama\\tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/film-o-rama/src/favicon.ico",
              "apps/film-o-rama/src/assets"
            ],
            "styles": [
              "apps/film-o-rama/src/styles/styles.scss",
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/film-o-rama/src/styles/vendors/normalize.css",
              "apps/film-o-rama/src/styles/fonts.scss",
              "apps/film-o-rama/src/styles/custom-reset.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/film-o-rama/src/environments/environment.ts",
                  "with": "apps/film-o-rama/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "film-o-rama:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "film-o-rama:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "film-o-rama:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/film-o-rama/src/test.ts",
            "polyfills": "apps/film-o-rama/src/polyfills.ts",
            "tsConfig": "apps\\film-o-rama\\tsconfig.spec.json",
            "karmaConfig": "apps\\film-o-rama\\karma.conf.js",
            "assets": [
              "apps/film-o-rama/src/favicon.ico",
              "apps/film-o-rama/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/film-o-rama/src/styles/styles.scss",
              "apps/film-o-rama/src/styles/vendors/normalize.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps\\film-o-rama\\tsconfig.app.json",
              "apps\\film-o-rama\\tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "film-o-rama-e2e": {
      "projectType": "application",
      "root": "apps\\film-o-rama-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps\\film-o-rama-e2e\\protractor.conf.js",
            "devServerTarget": "film-o-rama:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "film-o-rama:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps\\film-o-rama-e2e\\tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "film-o-rama"
}
